<?xml version='1.0' encoding='utf-8'?>
<widget id="tk.rakesh.cpmaassignment" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CPMA App Store</name>
    <description>
        CPMA Assignment
    </description>
    <author email="rakeshmr723@gmail.com" href="mycalc.tk">
        Rakesh M R
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="APP_SECRET" value="f54e5475-4a52-499f-8455-414aea74f0b8" />
        <preference name="APPCENTER_APP_SECRET" value="f54e5475-4a52-499f-8455-414aea74f0b8" />
        <resource-file src="google-services.json" target="app/google-services.json" />
</platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="logo.png"/>
</widget>
